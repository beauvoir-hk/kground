            <script>
                $(function() {
        
                    var ea = 2;
                    var mt = 0;
                    var time = { pause : 2000, motion : 500 };
        
                    var interv = null;
                    var $pr = $( "#pay_recent_wrap .list" );
                    var $prt = $pr.find( "table" );
                    var h = $prt.find("tr").height();
        
                    $pr.append($prt.clone());
                    $pr.height(h*ea);
                    function auto_start() {
                        interv = setInterval(function() {
                            mt -= h*ea;
                            $prt.animate({
                                marginTop: mt,
                            }, time.motion, function() {
                                var n = $pr.offset().top - ( $prt.offset().top + $prt.height() );
                                if( n >= 0 ) {
                                    mt = 0;
                                    $prt.css({marginTop: mt})
                                }
                            });
                        }, time.pause);
                    }
                    function auto_stop() {
                        clearInterval(interv);
                    }
        
                    $pr.on('mouseenter', auto_stop).on('mouseleave', auto_start);
                    auto_start();
                });
            </script>

