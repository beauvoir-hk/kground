/*---------------------------------------------------*/
/*  페이징 처리 함수                                 */
/*---------------------------------------------------*/
var _obj_form = "frm";
var _obj_paging = "area_page";
var _obj_list_title = "area_list_title";
var _area_data_id = "";
var _txt_style = "style=mso-number-format:'\@'";

/* ----------------------------------------------------------- */
//    함수명 : CallListData
//    설명 : 데이터 호출
//    파라미터 :
//        1. call_url = 호출 url
//        2. area_data_id = 데이터 매핑아이디 ( 분기처리용 값으로도 사용 )
/* ----------------------------------------------------------- */
var is_flag = false;

function CallListData(call_url, area_data_id) {
	if (is_flag && area_data_id !== "area_organization_list") return false;
	is_flag = true;

	_area_data_id = area_data_id;

	var obj_list_title_length = $("#area_list_title th").length;
	
	if (area_data_id === "area_product_buy_reservation_result") {
		obj_list_title_length = $("#area_list_title_2 th").length;
	}
	else if (area_data_id === "area_product_buy_reservation_waiting_result") {
		obj_list_title_length = $("#area_list_title_3 th").length;
	}

	var page = $.trim($("#page").val());
	var total_count = 0;
	var total_amount = 0;
	var all_data = false;
	if (area_data_id === "area_member_pay_all_list") {
		all_data = true;
	}

	$.ajax({
		type: "post",
		url: call_url,
		dataType: "json",
		async: true,
		cache: false,
		//headers: {
		//	RequestVerificationToken: $("#requestVerificationToken").val()
		//},
		beforeSend: function () {
			Loading(true);
		},
		data: $("#" + _obj_form).serialize(),
		success: function (data) {
			$("#" + area_data_id).children().remove();

			if (data !== null && data.toString() !== "") {
				total_count = data[0].TOTAL_COUNT;
				//if (area_data_id === "area_dw_list") {
				//	total_amount = data[data.length - 1].VIEW_SUM_AMOUNT;
				//}				
				
				if (!all_data) {
					PagingCall(page, total_count, call_url, area_data_id);
				}

				$.each(data, function (index) {
					DataMapping(area_data_id, data, index);
				});
			} else {
				var no_data_html = "";
				//var not_excel_count = $("#not_excel").length;
				//obj_list_title_length -= not_excel_count;

				if (obj_list_title_length > 0) {
					no_data_html = "<tr><td colspan='" + obj_list_title_length + "' style='text-align:center;' id='empty_list'>조회 내역이 없습니다.</td></tr>";
				}
				else {
					no_data_html = "<p style='text-align:center;' id='empty_list'>조회 내역이 없습니다.</p>";
				}

				if (!all_data) {
					PagingCall(1, 1, call_url, area_data_id);
				}
				$("#" + area_data_id).html(no_data_html);
			}

			if ($("#total_count").length > 0)				
			{
				$("#total_count").text(SetComma(total_count));
			}

			//if (area_data_id === "area_member_list" || area_data_id === "area_notice_list" || area_data_id === "area_dw_list" || area_data_id === "area_inquire_list" ||
			//	area_data_id === "area_package_list" || area_data_id === "area_reward_list" || area_data_id === "area_center_list" || area_data_id === "area_prefect_list" ||
			//	area_data_id === "area_fee_list" || area_data_id === "area_product_buy_reservation_list" || area_data_id === "area_product_transaction_list" ||
			//	area_data_id === "area_product_possession_list" || area_data_id === "area_point_list" || area_data_id === "area_product_sale_complete_list" || area_data_id === "area_package_use_list" ||
			//	area_data_id === "area_address_list" || area_data_id === "area_credit_list" || area_data_id === "area_lockup_list"
			//) {
			//	$("#total_count").text(SetComma(total_count));
			//	//if (area_data_id === "area_dw_list") {
			//	//	$("#total_amount").text(SetComma(total_amount));
			//	//}
			//	//SetTotalInfo(total_count);
			//}
		},
		error: function (request, textStatus, errorThrown) {
			Loading(false);
			is_flag = false;
			alert("통신중에러가 발생하였습니다. 재시도해 주세요.");
			//alert("code:" + request.status + "\n message: " + request.responseText + "\n error: " + errorThrown);
			//window.location.reload();
		},
		complete: function () {
			Loading(false);
			is_flag = false;
		}
	});
}

function DataMapping(area_data_id, data, index) {
	var view_leader = "";
	var admin_id = "";
	var memo = "";
	var append_data = "";
	var company_name = "";
	var coin_count = 1;
	var grade_turn = 0;
	var debug = $("#debug").val() === "True";
	let memberIdx = 0;

	if (area_data_id === "area_member_list") {
		company_name = data[index].COMPANY_NAME.toLowerCase();
		coin_count = parseInt($("#coin_count").val());
		
		append_data += "<tr>";
		append_data += "<td " + _txt_style + "><a class='g-type-link' title='회원상세정보로이동' href='/Admin/MemberDetail?member_idx=" + data[index].MEMBER_IDX + "'>" + data[index].MEMBER_IDX + "</a></td>";
		append_data += "<td " + _txt_style + ">" + data[index].JOIN_DATE + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].MEMBER_ID + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].MEMBER_NAME + "</td>";
		if (data[index].IS_GRADE_USE) {
			append_data += "<td " + _txt_style + ">" + data[index].GRADE_NAME + "</td>";
		}
		if (data[index].IS_SINGLE_PACKAGE_USE) {
			append_data += "<td " + _txt_style + ">" + data[index].MEMBER_PACKAGE_CODE_NAME + "</td>";
		}
		if (data[index].IS_USER_TURN_USE) {
			if (parseInt(data[index].MEMBER_TURN) > 0) {
				append_data += "<td " + _txt_style + " class='g-type-link' title='순번보기' onclick='GetUserTurnList(" + data[index].MEMBER_IDX + ")'>" + data[index].MEMBER_TURN + "</td>";
			}
			else {
				append_data += "<td " + _txt_style + ">" + data[index].MEMBER_TURN + "</td>";
            }
		}
		if (data[index].IS_EMAIL_AUTH) {
			append_data += "<td " + _txt_style + ">" + data[index].MEMBER_HP_NUMBER + "</td>";
		}
		if (data[index].IS_MOBILE_AUTH) {
			append_data += "<td " + _txt_style + ">" + data[index].MEMBER_EMAIL_ADDRESS + "</td>";
		}
		//if (company_name === "zeus") {
		//	append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].BANK_NAME + "</td>";
		//	append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].ACCOUNT_ADDRESS + "</td>";
		//}
		append_data += "<td " + _txt_style + ">" + data[index].MEMBER_STATE_CODE_NAME + "</td>";
		if (data[index].IS_MEMBER_OUT_APPLY === true) {
			append_data += "<td id='not_excel' class='ask-delete' onclick='MemberOutApplyConfirmProcess(" + data[index].MEMBER_IDX + ", \"" + data[index].MEMBER_ID + "\", \"" + data[index].MEMBER_HP_NUMBER + "\", \"" + data[index].MEMBER_EMAIL_ADDRESS + "\")'>탈퇴</td>";
		}
		else {
			append_data += "<td id='not_excel'> </td>";
		}

		if (company_name === "modua") {
			append_data += "<td id='not_excel'>";
			if (data[index].PRE_BOOL_1) {
				append_data += "<button type='button' id='btn_mining_distribution_" + data[index].MEMBER_IDX.toString() + "' class='g-btn style1' onclick='SetMiningDistributionSettingProcess(" + data[index].MEMBER_IDX + ", 0)'>취소</button>";
			}
			else {
				append_data += "<button type='button' id='btn_mining_distribution_" + data[index].MEMBER_IDX.toString() + "' class='g-btn' onclick='SetMiningDistributionSettingProcess(" + data[index].MEMBER_IDX + ", 1)'>지급</button>";
			}
			append_data += "</td>";
		}

		if (data[index].IS_MEMBER_TYPE_USE === true) {
			append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].MEMBER_TYPE_NAME + "</td>";
		}
		append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].COUNTRY_NAME + "</td>";

		//if (data[index].IS_MULTI_COIN_USE !== true) {	
		if (coin_count == 1) {
			append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_AMOUNT + "</td>";
		}
		else {
			for (var i = 0; i < coin_count; i++) {
				if (i == 0) {
					append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_AMOUNT_1 + "</td>";
				}
				else if (i == 1) {
					append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_AMOUNT_2 + "</td>";
				}
				else if (i == 2) {
					append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_AMOUNT_3 + "</td>";
				}
				else if (i == 3) {
					append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_AMOUNT_4 + "</td>";
				}
				else if (i == 4) {
					append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_AMOUNT_5 + "</td>";
				}
				else if (i == 5) {
					append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_AMOUNT_6 + "</td>";
				}
				else if (i == 6) {
					append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_AMOUNT_7 + "</td>";
				}
				else if (i == 7) {
					append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_AMOUNT_8 + "</td>";
				}
				else if (i == 8) {
					append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_AMOUNT_9 + "</td>";
				}
				else if (i == 9) {
					append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_AMOUNT_10 + "</td>";
				}
			}
		}
		//}

		if (data[index].IS_RECOMMENDER_USE === true) {
			append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].RECOMMENDER_ID + "</td>";
			append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].RECOMMENDER_NAME + "</td>";
		}
		if (data[index].IS_SUPPORT_USE === true || data[index].COMPANY_NAME.toLowerCase() === "uqt") {
			append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].SUPPORT_ID + "</td>";
			append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].SUPPORT_NAME + "</td>";
		}
		if (data[index].IS_PRODUCT_SALE_USE === true) {
			append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_PRODUCT_BUY_WAY_QUANTITY + "</td>";
		}
		if (data[index].IS_POINT_USE === true) {
			append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_POINT_AMOUNT + "</td>";
		}
		if (data[index].IS_POINT_MANAGE_USE) {
			append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_POINT + "</td>";
		}
		if (data[index].IS_PACKAGE_USE && data[index].IS_POINT_MANAGE_USE) {
			if (company_name === "step") {
				append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_PACKAGE_POINT_QUANTITY + "</td>";
				append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_PACKAGE_POINT_AMOUNT + "</td>";
			}
			else {
				append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_PACKAGE_POINT_AMOUNT + "</td>";
			}
		}
		//if (company_name === "zeus") {
		//	append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_CREDIT_COUNT + "</td>";
		//	append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_PRODUCT_COUNT + "</td>";
		//	append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_PRODUCT_ORIGINAL_AMOUNT + "</td>";
		//	append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_PRODUCT_RESERVATION_WAITING_COUNT + "</td>";
		//	append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].VIEW_SUM_DOWN_RECOMMENDER_PRODUCT_SALE_COMPLETE_COUNT + "</td>";
		//}
		append_data += "</tr>";
	}
	else if (area_data_id === "area_notice_list") {
		append_data += "<tr>";
		append_data += "<td " + _txt_style + ">" + data[index].NO + "</td>";
		append_data += "<td " + _txt_style + "><a class='g-type-link' title='상세보기' href='/NoticeBoard/NoticeEdit?notice_idx=" + data[index].NOTICE_IDX + "&notice_code=" + data[index].NOTICE_CODE + "'>" + data[index].TITLE + "</a></td>";
		append_data += "<td " + _txt_style + ">" + data[index].ADMIN_ID + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].INS_DATE + "</td>";
		append_data += "</tr>";
	}
	else if (area_data_id === "area_inquire_list") {
		var is_reply = data[index].IS_REPLY;
		var reply_state_name = "";
		var is_reply_class = "";
		
		if (is_reply === true) {
			is_reply_class = "g-success";
			reply_state_name = "답변완료";
		}
		else {
			is_reply_class = "g-pending";
			reply_state_name = "답변대기";
		}

		append_data += "<tr>";
		append_data += "<td " + _txt_style + ">" + data[index].NO + "</td>";
		if (data[index].TYPE_CODE_NAME !== "") {
			append_data += "<td " + _txt_style + ">" + data[index].TYPE_CODE_NAME + "</td>";
		}
		else {
			append_data += "<td></td>";
		}
		append_data += "<td " + _txt_style + "><a class='g-type-link' title='상세보기' href='/NoticeBoard/InquireDetail?notice_idx=" + data[index].NOTICE_IDX + "'>" + data[index].TITLE + "</a></td>";
		append_data += "<td " + _txt_style + "><a class='g-type-link' title='회원상세정보로이동' href='/Admin/MemberDetail?member_idx=" + data[index].MEMBER_IDX + "'>" + data[index].MEMBER_ID + "</a></td>";
		append_data += "<td " + _txt_style + ">" + data[index].INS_DATE + "</td>";
		append_data += "<td " + _txt_style + " class='" + is_reply_class + "'>" + reply_state_name + "</td>";
		append_data += "</tr>";
	}
	else if (area_data_id === "area_dw_list") {
		var state_code = data[index].STATE_CODE;
		var process_type_code = data[index].PROCESS_TYPE_CODE;
		var process_type_code_name = data[index].PROCESS_TYPE_CODE_NAME;
		var pre_column_01 = data[index].PRE_COLUMN_01;
		var pre_column_02 = data[index].PRE_COLUMN_02;
		var pre_column_03 = data[index].PRE_COLUMN_03;
		var deposit_amount = parseFloat(data[index].DEPOSIT_AMOUNT);
		var amount = data[index].AMOUNT;
		var original_amount = data[index].ORIGINAL_AMOUNT;
		var view_original_amount = data[index].VIEW_ORIGINAL_AMOUNT;
		var view_amount = data[index].VIEW_AMOUNT;
		var view_deposit_amount = data[index].VIEW_DEPOSIT_AMOUNT;
		var txid = data[index].TXID;
		var member_id = data[index].MEMBER_ID;
		var is_change_transaction = data[index].IS_CHANGE_TRANSACTION;
		var change_amount = data[index].VIEW_CHANGE_AMOUNT;
		var coin_type = data[index].COIN_TYPE;
		admin_id = data[index].ADMIN_ID;
		memo = data[index].ORIGINAL_MEMO;
		var memoView = "";
		var memo_view_title = "";
		if (amount === 0) {
			view_amount = view_original_amount;
		}
		console.log(memo);
		var state_code_name_class = "";
		if (state_code === "complete") {
			state_code_name_class = "g-success";
		}
		else if (state_code === "waiting") {
			state_code_name_class = "g-pending";
			if (is_change_transaction === true) {
				state_code_name_class += " g-type-link";
			}
		}

		append_data += "<tr>";
		append_data += "<td " + _txt_style + ">" + data[index].MEMBER_PAY_IDX + "</span></td>";
		append_data += "<td " + _txt_style + ">" + data[index].UPD_DATE + "</span></td>";
		append_data += "<td " + _txt_style + ">" + data[index].MEMBER_NAME + "</span></td>";
		if (process_type_code === "deposit") {
			if (pre_column_01 === "") {
				append_data += "<td " + _txt_style + "><a class='g-type-link' title='회원상세정보로이동' href='/Admin/MemberDetail?member_idx=" + data[index].MEMBER_IDX + "'>" + data[index].MEMBER_ID + "</a></td>";
			}
			else {
				if (deposit_amount === 0) {
					append_data += "<td " + _txt_style + "><a class='g-type-link' title='회원상세정보로이동' href='/Admin/MemberDetail?member_idx=" + data[index].MEMBER_IDX + "'>" + data[index].MEMBER_ID + "</a></td>";
				}
				else {
					append_data += "<td " + _txt_style + "><a class='g-type-link' title='회원상세정보로이동' href='/Admin/MemberDetail?member_idx=" + data[index].MEMBER_IDX + "'>" + data[index].MEMBER_ID + "</a><span class='g-sub-content'>Contact : " + pre_column_01 + "</span></td>";
				}
			}

			if (txid === "") {
				if (is_change_transaction === true) {
					if (state_code === "complete") {
						memo_view_title = "TXID";
						memoView += txid + "<br />";
					}
					else {
						memo_view_title = "info";
					}
					
					if (coin_type.toLowerCase() === "xrp") {
						memoView += "Address : " + pre_column_01.split("_")[0];
						memoView += "<br />Destination Code : " + pre_column_01.split("_")[1];
					}
					else {
						memoView += "Address : " + pre_column_01;
					}
					append_data += "<td " + _txt_style + "><span class='g-type-link' onclick='MemoView(\"" + memo_view_title + "\", \"" + memoView + "\")'>" + process_type_code_name + "</span></td>";
				}
				else {
					if (pre_column_01 !== "") {
						append_data += "<td " + _txt_style + "><span class='g-type-link' onclick='MemoView(\"id\", \"" + pre_column_01 + "\")'>" + process_type_code_name + "</span></td>";
					}
					else {
						append_data += "<td " + _txt_style + ">" + process_type_code_name + "</td>";
					}
				}
			}
			else {
				if (deposit_amount === 0) {
					txid = txid + "<br />Address : " + pre_column_01;
					append_data += "<td " + _txt_style + "><span class='g-type-link' onclick='MemoView(\"TXID\", \"" + txid + "\")'>" + process_type_code_name + "</span></td>";
				}
				else {
					if (pre_column_02 !== "") {
						txid = txid + "<br />Address(" + pre_column_03 + ") : " + pre_column_02;
					}
					if (pre_column_01 !== "") {
						txid = txid + "<br />Contect : " + pre_column_01;
					}
					append_data += "<td " + _txt_style + "><span class='g-type-link' onclick='MemoView(\"TXID\", \"" + txid + "\")'>" + process_type_code_name + "</span></td>";
				}
			}
			append_data += "<td " + _txt_style + ">" + coin_type + "</td>";
			if (deposit_amount === 0) {
				if (is_change_transaction === true) {
					if (coin_type.toLowerCase() !== pre_column_02.toLowerCase()) {
						append_data += "<td " + _txt_style + ">" + view_amount + "<span class='g-sub-content'>" + pre_column_02.toUpperCase() + " : " + change_amount + "</span></td>";
					}
					else {
						append_data += "<td " + _txt_style + ">" + view_amount + "</td>";
					}
				}
				else {
					append_data += "<td " + _txt_style + ">" + view_amount + (data[index].IS_USD_SHOW ? "(" + SetComma(data[index].USD_CONVERSION_AMOUNT) + "$)" : "") + "</td>";
				}				
			}
			else {
				append_data += "<td " + _txt_style + ">" + view_amount + (data[index].IS_USD_SHOW ? "(" + SetComma(data[index].USD_CONVERSION_AMOUNT) + "$)" : "");
				append_data += "<span class='g-sub-content'>" + pre_column_03 + " : " + view_deposit_amount + "</span></td>";
			}
		}
		else if (process_type_code === "withdraw") {
			append_data += "<td " + _txt_style + "><a class='g-type-link' title='회원상세정보로이동' href='/Admin/MemberDetail?member_idx=" + data[index].MEMBER_IDX + "'>" + member_id + "</a></td>";
			if (data[index].IS_ADDRESS_WITHDRAW === true) {
				if (is_change_transaction === true) {
					if (state_code === "complete") {
						memo_view_title = "TXID";
						memoView += txid + "<br />";
					}
					else {
						memo_view_title = "info";
					}

					if (coin_type.toLowerCase() === "xrp") {
						memoView += "Address : " + pre_column_01.split("_")[0];
						memoView += "<br />Destination Code : " + pre_column_01.split("_")[1];
					}
					else {
						memoView += "Address : " + pre_column_01;
					}
					append_data += "<td " + _txt_style + "><span class='g-type-link' onclick='MemoView(\"info\", \"" + memoView + "\")'>" + process_type_code_name + "</span></td>";
				}
				else {
					if (txid === "") {
						txid = "Address : " + pre_column_01;
					}
					else {
						txid = txid + "<br />Address : " + pre_column_01;
					}

					append_data += "<td " + _txt_style + "><span class='g-type-link' onclick='MemoView(\"info\", \"" + txid + "\")'>" + process_type_code_name + "</span></td>";
				}
			}
			else {
				if (txid === "") {
					if (pre_column_01 !== "") {
						append_data += "<td " + _txt_style + "><span class='g-type-link' onclick='MemoView(\"id\", \"" + pre_column_01 + "\")'>" + process_type_code_name + "</span></td>";
					}
					else {
						append_data += "<td " + _txt_style + ">" + process_type_code_name + "</td>";
					}
				}
			}

			append_data += "<td " + _txt_style + ">" + coin_type + "</td>";
			
			if (is_change_transaction === true) {
				append_data += "<td " + _txt_style + ">" + view_amount;
				if (data[index].WITHDRAW_FEES_AMOUNT > 0) {
					append_data += "<span class='g-sub-content'>Amount : " + data[index].VIEW_WITHDRAW_AMOUNT + "</span>";
					append_data += "<span class='g-sub-content'>Fee : " + data[index].VIEW_WITHDRAW_FEES_AMOUNT + "</span>";
				}
				if (coin_type.toLowerCase() !== pre_column_02.toLowerCase() && state_code === "complete") {
					append_data += "<span class='g-sub-content'>" + pre_column_02.toUpperCase() + " : " + change_amount + "</span></td>";
				}
			}
			else {
				append_data += "<td " + _txt_style + ">" + view_amount + (data[index].IS_USD_SHOW ? "(" + SetComma(data[index].USD_CONVERSION_AMOUNT) + "$)" : "");
				if (data[index].WITHDRAW_FEES_AMOUNT > 0) {
					append_data += "<span class='g-sub-content'>Amount : " + data[index].VIEW_WITHDRAW_AMOUNT + "</span>";
					append_data += "<span class='g-sub-content'>Fee : " + data[index].VIEW_WITHDRAW_FEES_AMOUNT + "</span>";
				}
				append_data += "</td>";
			}			
		}
		else if (process_type_code === "card_charge") {
			memoView = "Card Number : " + pre_column_01 + "<br />Content : " + pre_column_02;
