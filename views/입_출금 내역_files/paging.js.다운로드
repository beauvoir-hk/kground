				append_data += "<div class='processing row'><div class='col-6 pr-pl-none'><button type='button' class='g-btn btn-style' onclick='PayApplyConfirmProcess(" + data[index].MEMBER_PAY_IDX + ", \"" + edit_state_code + "\", \"" + state_code + "\")'>수락</button></div>";
				append_data += "<div class='col-6 pr-none'><button type='button' class='g-btn style1 g-cancel' onclick='PayApplyConfirmProcess(" + data[index].MEMBER_PAY_IDX + ", \"cancel\", \"" + state_code + "\")'>거절</button></div></div>";
			}
			else {
				append_data += "<span class='g-type-link' title='취소' onclick='PayApplyConfirmProcess(" + data[index].MEMBER_PAY_IDX + ", \"cancel\", \"" + state_code + "\")'>취소</span>";
			}
			append_data += "</td>";
		}
		else {
			append_data += "<td id='not_excel'></td>";
		}
		append_data += "<td " + _txt_style + " id='hidden_column' class='hide'>" + data[index].TXID + "</td>";
		append_data += "</tr>";
