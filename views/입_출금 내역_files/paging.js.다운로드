		append_data += "<td " + _txt_style + ">" + data[index].SALE_COMPLETE_DATE + "</td>";
		append_data += "<td " + _txt_style + "><a class='g-type-link' title='회원상세정보로이동' href='/Admin/MemberDetail?member_idx=" + data[index].MEMBER_IDX + "'>" + data[index].MEMBER_ID + "</a></td>";
		append_data += "<td " + _txt_style + ">" + data[index].TYPE_CODE_NAME + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].VIEW_ORIGINAL_QUANTITY + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].STATE_CODE_NAME + "</td>";
		if (admin_id === "") {
			append_data += "<td " + _txt_style + "><span></span></td>";
		}
		else {
			append_data += "<td " + _txt_style + "><span class='g-type-link' title='사유확인' onclick='MemoView(\"memo\", \"" + memo + "\")'>" + admin_id + "</span></td>";
		}
		append_data += "</tr>";
	}
	else if (area_data_id === "area_point_manage") {
		append_data += "<tr>";
		append_data += "<td>" + data[index].POINT_MANAGE_IDX + "</td>";
		append_data += "<td><a class='g-type-link' title='포인트상세정보로이동' href='/Manage/PointDetail?point_manage_idx=" + data[index].POINT_MANAGE_IDX + "'>" + data[index].POINT_COMPANY_NAME + "</a></td>";
		append_data += "<td>" + data[index].POINT_NAME + "</td>";
		append_data += "<td>" + (data[index].IS_INTERLOCK ? "ON" : "OFF") + "</td>";
		append_data += "<td>" + (data[index].IS_EXP ? "ON" : "OFF") + "</td>";
		append_data += "</tr>";
	}
	else if (area_data_id === "area_lockup_list") {
		append_data += "<tr>";
		append_data += "<td " + _txt_style + "><a class='g-type-link' title='상세정보로이동' href='/List/LockupDetail/" + data[index].LOCK_IDX.toString() + "'>" + data[index].REFERENCE_KEY + "</a></td>";
		append_data += "<td " + _txt_style + ">" + data[index].INS_DATE + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].MEMBER_ID + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].COIN_TYPE + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].VIEW_AMOUNT + "</td>";
		if (data[index].COMPANY_NAME.toLowerCase() === "oec") {
			append_data += "<td " + _txt_style + ">" + data[index].FIRST_TERMINATE_DATE + "</td>";
		}
		append_data += "<td " + _txt_style + ">매달 " + data[index].TERMINATE_DATE + "일</td>";
		append_data += "<td " + _txt_style + ">" + data[index].TERMINATE_RATE + "%</td>";
		append_data += "<td " + _txt_style + ">" + data[index].VIEW_TERMINATE_AMOUNT + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].VIEW_REMAINS_AMOUNT + "</td>";
		append_data += "</tr>";
	}
	else if (area_data_id === "area_master_ip_list") {
		append_data += "<tr>";
		append_data += "<td " + _txt_style + ">" + data[index].COMPANY_NAME + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].PRE_COLUMN_01 + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].PRE_COLUMN_02 + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].DOMAIN_NAME + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].ADMIN_DOMAIN_NAME + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].POLICY_INS_DATE + "</td>";
		append_data += "<td " + _txt_style + " id='not_excel'><button type='button' class='g-btn style1 g-cancel-type2 g-w-64' onclick='DeleteIpProcess(" + data[index].COMPANY_IDX + ", \"" + data[index].PRE_COLUMN_01 + "\")'>삭제</button></td>";
		append_data += "</tr>";
	}
	else if (area_data_id === "area_auth_list") {
		append_data += "<tr>";
		append_data += "<td>" + data[index].AUTH_SOLT_CODE_NAME + "</td>";
		append_data += "<td>" + data[index].AUTH_RECEIVE_INFO + "</td>";
		append_data += "<td>" + data[index].AUTH_TEXT + "</td>";
		append_data += "<td>" + data[index].INS_DATE + "</td>";
		append_data += "</tr>";
	}
	else if (area_data_id === "area_ip_list") {
		append_data += "<tr>";
		append_data += "<td " + _txt_style + ">" + data[index].PRE_COLUMN_01 + "</td>";
		append_data += "</tr>";
	}
	else if (area_data_id === "area_cs_list") {
		append_data += "<tr>";
		append_data += "<td " + _txt_style + ">" + data[index].CS_IDX + "</td>";
		append_data += "<td>";
		if (data[index].IMG_URL_1 != "" || data[index].IMG_URL_2 != "" || data[index].IMG_URL_3 != "") {
			append_data += "<div class='d-flex align-items-center justify-content-center'><span class='g-type-link' onclick='PageMoveHref(\"/List/CsInfo/" + data[index].CS_IDX + "\")'>" + data[index].TITLE + "</span><span class='material-icons'>image</span></div>";
		}
		else {
			append_data += "<span class='g-type-link' onclick='PageMoveHref(\"/List/CsInfo/" + data[index].CS_IDX + "\")'>" + data[index].TITLE + "</span>";
		}
		append_data += "</div>";
		append_data += "</td>";
		append_data += "<td>" + data[index].REG_ADMIN_ID + "</td>";
		append_data += "<td>";
		append_data += "<span class=" + (data[index].IS_REPLY ? 'complete' : 'waiting') + ">" + (data[index].IS_REPLY ? "답변완료" : "답변대기") + "</span>";
		append_data += "</td>";
		append_data += "<td>" + data[index].INS_DATE + "</td>";
		append_data += "<td id='not_excel'>";
		if (data[index].REGIST_ADMIN_IDX === data[index].REGIST_ADMIN_IDX && !data[index].IS_REPLY) {
			append_data += "<button type='button' class='g-btn style1 g-cancel-type2' onclick='DeleteCsInfoProcess(" + data[index].CS_IDX + ")'>삭제</button>";
		}
		else {
			append_data += "-";
		}
		append_data += "</td>";
		append_data += "</tr>";
	}
	else if (area_data_id === "area_master_cs_list") {
		append_data += "<tr>";
		append_data += "<td " + _txt_style + ">" + data[index].CS_IDX + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].COMPANY_NAME + "</td>";
		append_data += "<td>";
		if (data[index].IMG_URL_1 != "" || data[index].IMG_URL_2 != "" || data[index].IMG_URL_3 != "") {
			append_data += "<div class='d-flex align-items-center justify-content-center'><span class='g-type-link' onclick='PageMoveHref(\"/Master/CsInfo/" + data[index].CS_IDX + "/" + data[index].COMPANY_IDX + "\")'>" + data[index].TITLE + "</span><span class='material-icons'>image</span></div>";
		}
		else {
			append_data += "<span class='g-type-link' onclick='PageMoveHref(\"/Master/CsInfo/" + data[index].CS_IDX + "/" + data[index].COMPANY_IDX + "\")'>" + data[index].TITLE + "</span>";
		}
		append_data += "</td>";
		append_data += "<td>" + data[index].REG_ADMIN_ID + "</td>";
		append_data += "<td>";
		append_data += "<span class=" + (data[index].IS_REPLY ? 'complete' : 'waiting') + ">" + (data[index].IS_REPLY ? "답변완료" : "답변대기") + "</span>";
		append_data += "</td>";
		append_data += "<td>" + data[index].INS_DATE + "</td>";
		append_data += "</tr>";
	}
	else if (area_data_id === "area_master_company_list") {
		var company_type_code_name = "지갑";
		if (data[index].COMPANY_TYPE_CODE === "pay") {
			company_type_code_name = data[index].IS_PRODUCT_SALE_USE ? "P2P" : "전산";
        }

		append_data += "<tr>";
		append_data += "<td " + _txt_style + ">" + data[index].COMPANY_IDX + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].COMPANY_NAME + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].INS_DATE + "</td>";
		append_data += "<td " + _txt_style + "><span class='g-type-link' onclick='OpenNewPage(\"" + (debug ? "http://" : "https://") + data[index].DOMAIN_NAME + "\")'>" + data[index].DOMAIN_NAME + "</span></td>";
		append_data += "<td " + _txt_style + "><span class='g-type-link' onclick='OpenNewPage(\"" + (debug ? "http://" : "https://") + data[index].ADMIN_DOMAIN_NAME + "\")'>" + data[index].ADMIN_DOMAIN_NAME + "</span></td>";
		append_data += "<td " + _txt_style + ">" + company_type_code_name + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].SOLUTION_VERSION + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].COMPANY_MEMBER_ID + "</td>";
		append_data += "<td " + _txt_style + ">" + (data[index].IS_MOBILE_AUTH ? "사용" : "미사용") + "</td>";
		append_data += "<td " + _txt_style + ">" + (data[index].IS_EMAIL_AUTH ? "사용" : "미사용") + "</td>";
		append_data += "<td " + _txt_style + ">" + (data[index].IS_RECOMMENDER_USE ? "사용" : "미사용") + "</td>";
		append_data += "<td " + _txt_style + ">" + (data[index].IS_SUPPORT_USE ? "사용" : "미사용") + "</td>";
		append_data += "<td " + _txt_style + ">" + (data[index].IS_AVATAR ? "사용" : "미사용") + "</td>";
		append_data += "<td " + _txt_style + ">" + (data[index].IS_PREFECT_USE ? "사용" : "미사용") + "</td>";
		append_data += "<td " + _txt_style + ">" + (data[index].IS_CENTER_USE ? "사용" : "미사용") + "</td>";
		append_data += "<td " + _txt_style + ">" + (data[index].IS_DEL ? "종료" : "운영중") + "</td>";
		append_data += "</tr>";
	}
	else if (area_data_id === "area_package_refund_apply_list") {
		memberIdx = data[index].MEMBER_IDX;
		append_data += "<tr>";
		append_data += "<td " + _txt_style + ">" + data[index].REFUND_APPLY_DATE + "</td>";
		append_data += "<td " + _txt_style + "><a class='g-type-link' title='회원상세정보로이동' href='/Admin/MemberDetail?member_idx=" + memberIdx + "'>" + data[index].MEMBER_ID + "</a></td>";
		append_data += "<td " + _txt_style + ">" + SetComma(data[index].SUM_PACKAGE_AMOUNT) + "</td>";
		append_data += "<td " + _txt_style + ">" + SetComma(data[index].REFUND_AMOUNT) + "</td>";
		append_data += "<td " + _txt_style + ">" + SetComma(data[index].REFUND_FEE_AMOUNT) + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].REFUND_RATE + "</td>";
		append_data += "<td id='not_excel'>";
		append_data += "<div class='processing row'><div class='col-6 pr-pl-none'><button type='button' class='g-btn btn-style' onclick='PackageRefundProcess(" + memberIdx + ", 2, " + JSON.parse(data[index].IS_CENTER_LEADER) + ")'>수락</button></div>";
		append_data += "<div class='col-6 pr-none'><button type='button' class='g-btn style1 g-cancel' onclick='PackageRefundProcess(" + memberIdx + ", 0, " + JSON.parse(data[index].IS_CENTER_LEADER) + ")'>거절</button></div></div>";
		append_data += "</td>";
		append_data += "</tr>";
	}
	else if (area_data_id === "area_organization_list") {
		memberIdx = data[index].MEMBER_IDX;

		var isCompanyMember = data[index].MEMBER_TYPE === "company";
		var isWithdrawPossible = data[index].IS_WITHDRAW_POSSIBLE;
		var memberIdx_check_box_id = "memberIdx_" + memberIdx;

		append_data += "<tr>";
		append_data += "<td id='not_excel'>";
		if (!isCompanyMember) {
			append_data += "<label class='g-checkbox' for='" + memberIdx_check_box_id + "'>";
			append_data += "<input type='checkbox' class='inputCk' name='memberIdxs' id='" + memberIdx_check_box_id + "' value='" + memberIdx + "' data-iswithdrawpossible='" + isWithdrawPossible + "' />";
			append_data += "<span class='checkmark'></span></label>";
		}
		append_data += "</td>";
		append_data += "<td " + _txt_style + ">";
		if (isCompanyMember) {
			append_data += data[index].MEMBER_ID;
		}
		else {
			append_data += "<a class='g-type-link' title='회원상세정보로이동' href='/Admin/MemberDetail?member_idx=" + memberIdx + "'>" + data[index].MEMBER_ID + (data[index].IS_MEMBER_OUT ? "(탈퇴)" : "") + "</a>";
		}
		append_data += "</td>";
		append_data += "<td>" + data[index].TURN + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].MEMBER_NAME + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].MEMBER_HP_NUMBER + "</td>";
		append_data += "<td " + _txt_style + ">" + (isCompanyMember ? "" : data[index].GRADE_NAME) + "</td>";
		append_data += "<td>" + (isCompanyMember ? "" : (isWithdrawPossible ? "가능" : "불가능")) + "</td>";
		append_data += "<td " + _txt_style + ">" + (isCompanyMember ? "" : SetComma(data[index].SUM_AMOUNT)) + "</td>";
		append_data += "<td " + _txt_style + ">" + (isCompanyMember ? "" : SetComma(data[index].SUM_POINT_AMOUNT)) + "</td>";
		append_data += "<td " + _txt_style + ">" + (isCompanyMember ? "" : SetComma(data[index].POINT_PACKAGE_REMAINS_AMOUNT)) + "</td>";
		append_data += "<td " + _txt_style + ">" + (isCompanyMember ? "" : data[index].JOIN_DATE) + "</td>";
		append_data += "</tr>";
	}
	else if (area_data_id === "area_nft_transaction_list") {
		memberIdx = data[index].MEMBER_IDX;
		var logIdx = data[index].LOG_IDX;

		append_data += "<tr id='nft_transaction_" + logIdx + "'>";
		append_data += "<td>" + data[index].TOKEN_ID + "</td>";
		append_data += "<td " + _txt_style + "><a class='g-type-link' title='회원상세정보로이동' href='/Admin/MemberDetail?member_idx=" + memberIdx + "'>" + data[index].MEMBER_ID + "</a></td>";
		append_data += "<td>" + data[index].TYPE_CODE_NAME + "</td>";
		append_data += "<td><span class='" + data[index].STATE_CODE + "'>" + data[index].STATE_CODE_NAME + "</span></td>";
		append_data += "<td " + _txt_style + ">" + (data[index].TYPE_CODE === "buy" ? data[index].VIEW_BUY_AMOUNT : "") + "</td>";
		append_data += "<td>" + data[index].BUY_SYMBOL + "</td>";
		if (data[index].SEND_MEMBER_IDX == 0) {
			append_data += "<td></td>";
		}
		else {
			append_data += "<td " + _txt_style + "><a class='g-type-link' title='회원상세정보로이동' href='/Admin/MemberDetail?member_idx=" + data[index].SEND_MEMBER_IDX + "'>" + data[index].SEND_MEMBER_ID + "</a></td>";
		}
		append_data += "<td " + _txt_style + ">" + data[index].WITHDRAWAL_ADDRESS + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].UPD_DATE + "</td>";
		append_data += "<td " + _txt_style + ">" + data[index].ADMIN_ID + "</td>";
		append_data += "<td id='not_excel'>";
		if (data[index].TYPE_CODE === "withdraw" && data[index].STATE_CODE === "apply") {
			append_data += "<button type='button' class='g-btn style1' onclick='NFTWithdrawProcess(" + logIdx + ", \"complete\")'>완료</button>";
			append_data += "<button type='button' class='g-btn style1 g-cancel' onclick='NFTWithdrawProcess(" + logIdx + ", \"cancel\")'>취소</button>";
		}
		append_data += "</td>";
		append_data += "</tr>";
	}
	else if (area_data_id === "area_member_nft_list") {
		memberIdx = data[index].MEMBER_IDX;
		admin_id = data[index].ADMIN_ID;
		memo = data[index].MEMO;

		append_data += "<tr>";
		append_data += "<td>" + data[index].PRE_INT_COLUMN_01 + "</td>";
		append_data += "<td " + _txt_style + "><a class='g-type-link' title='회원상세정보로이동' href='/Admin/MemberDetail?member_idx=" + memberIdx + "'>" + data[index].MEMBER_ID + "</a></td>";
		if (admin_id === "") {
			append_data += "<td></td>";
		}
		else {
			if (memo === "") {
				append_data += "<td " + _txt_style + ">" + admin_id + "</td>";
			}
			else {
				append_data += "<td " + _txt_style + "><span class='g-type-link' title='사유확인' onclick='MemoView(\"memo\", \"" + memo + "\")'>" + admin_id + "</span></td>";
			}
		}
		append_data += "<td id='not_excel'><button type='button' class='g-btn style1 g-cancel-type2' onclick='NFTBurnProcess(" + data[index].IDX + ", " + memberIdx + ")'>소각</button></td>";
		append_data += "</tr>";
	}

	$("#" + area_data_id).append(append_data);
}

/* ----------------------------------------------------------- */
//    함수명 : PagingCall
//    설명 : 번호이동식 페이지 계산
//    파라미터 : 
//        1. page = 현재페이지 파라미터명
//        2. total_count = 데이터 총 개수
//        3. call_url = 요청 URL
//        4. area_data_id = 데이터가 노출될 태그아이디
/* ----------------------------------------------------------- */
function PagingCall(page, totalCount, call_url, area_data_id) {
	var blockSize = 10;
	var pageSize = 200;
	//if (area_data === "area_all_member_list") {
	//	pageSize = 20;
	//}
	// 전체 페이지 확인    
	var divisionCount = parseInt(totalCount) / parseInt(pageSize);                          // 전체 페이지 / 노출 개수 (소수점 까지 표시됨)
	var divisionRestLen = parseInt(totalCount) % parseInt(pageSize);                        // 전체 페이지 / 노출 개수 나머지
	//var divisionLen = divisionCount.toString().length;                                      // 1보다 클 경우 소수점 있음으로 간주 (2자리 소수점때문에 string으로 확인
	var totalPageCount = 1;
	var divisionAddCount = 0;
	if (divisionRestLen > 0) {
		divisionAddCount = 1;
	}
	totalPageCount = parseInt(divisionCount) + divisionAddCount;
	//    if (divisionLen > 1) {                                                                  // 총 노출 순번
	//        //alert(divisionCount);
	//        totalPageCount = divisionCount + 1;
	//        totalPageCount = parseInt(totalPageCount);
	//    }
	//    else if (divisionLen == 1) {
	//        totalPageCount = parseInt(divisionCount);
	//    }

	// 페이지 시작, 끝 번호 설정
	var maxPageAddCount = blockSize - 1;                                                    // 노출될 순번 최종 개수 (예) 1 ~ 10이면 9)
	var pagingMinDivisionCount = parseInt(page) / blockSize;                             // 현재페이지 노출 최초 번호
	var pagingMinRestCount = parseInt(page) % blockSize;                                 // 현재페이지 노출 최초 번호
	var pagingMinCount = 1;                                                                 // 노출될 페이징 최초 번호
	var pagingMaxCount = 1;                                                                 // 노출될 페이징 최종 번호

	if (parseInt(pagingMinDivisionCount) >= 1) {                                            // 현재 페이지가 노출되야될 페이지 순번 보다 클 경우
		pagingMinCount = (parseInt(pagingMinDivisionCount) * blockSize) + 1;
	}
	if (parseFloat(pagingMinRestCount) === 0) {
		pagingMinCount = parseInt(pagingMinCount) - blockSize;
	}

	if ((totalPageCount - pagingMinCount) < maxPageAddCount) {
		maxPageAddCount = totalPageCount - pagingMinCount;
	}
	if ((pagingMinCount + maxPageAddCount) <= totalPageCount) {                             // 노출될 페이징 최종 번호 총 노출순번보다 같거나 작을 경우 해당 순번 끝까지 노출
		pagingMaxCount = pagingMinCount + maxPageAddCount;
	} else if (parseFloat(pagingMinCount) === parseFloat(totalPageCount)) {                                          // 시작 페이지와 최종 페이지가 같을경우 최종페이지만 노출
		pagingMaxCount = totalPageCount;
	}
	//    else {                                                                                // 클 경우 해당순번 나머지 노출
	//        pagingMaxCount = (totalPageCount - pagingMinCount) + 1;
	//    }

	//alert(pagingMinCount + "/" + maxPageAddCount + "/" + totalPageCount + "/" + pagingMinCount);

	// 페이징 비활성화 로직
	var prevPage = parseInt(page) - 1;
	var nextPage = parseInt(page) + 1;
	var firstMoveType = 0;
	var prevMoveType = 0;
	var moveType = 0;
	var nextMoveType = 0;
	var lastMoveType = 0;

	// 첫 페이지일 경우 이전페이지 비활성 처리
	if (parseFloat(page) === 1) {
		firstMoveType = 1;
		prevMoveType = 1;
	} else {
		firstMoveType = 0;
		prevMoveType = 0;
	}

	// 마지막 페이지일 경우 다음페이지 비활성화 처리
	if (parseFloat(page) === totalPageCount) {
		nextMoveType = 1;
		lastMoveType = 1;
	} else {
		nextMoveType = 0;
		lastMoveType = 0;
	}

	// 페이징 노출 처리
	$("#" + _obj_paging).children().remove();
	var append_data = "";

	if (prevMoveType === 0) {
		append_data += "<li class='page-item'><a class='page-link' href='javascript:MovePage(" + 1 + ", " + prevMoveType + ", \"" + call_url + "\", \"" + area_data_id + "\");' aria-label='First'><i class='fas fa-angle-double-left fa-lg'></i></a></li>";
		append_data += "<li class='page-item'><a class='page-link' href='javascript:MovePage(" + prevPage + ", " + prevMoveType + ", \"" + call_url + "\", \"" + area_data_id + "\");' aria-label='Previous'><i class='fas fa-angle-left fa-lg'></i></a></li>";
	}
	else {
		append_data += "<li class='page-item disabled'><a class='page-link' aria-label='First'><i class='fas fa-angle-double-left fa-lg'></i></a></li>";
		append_data += "<li class='page-item disabled'><a class='page-link' aria-label='Previous'><i class='fas fa-angle-left fa-lg'></i></a></li>";
	}

	for (var i = pagingMinCount; i <= pagingMaxCount; i++) {
		var sel_class = "page-item";
		if (parseFloat(page) === i) {
			sel_class = "page-item active";
			moveType = 1;
		} else {
			moveType = 0;
		}

		if (parseFloat(moveType) === 0) {
			append_data += "<li class='" + sel_class + "'><a class='page-link' href='javascript:MovePage(" + i + ", " + moveType + ", \"" + call_url + "\", \"" + area_data_id + "\");'>" + i + "</a></li>";
		} else {
			append_data += "<li class='" + sel_class + "'><a class='page-link'>" + i + "</a></li>";
		}
	}

	if (nextMoveType === 0) {
		append_data += "<li class='page-item'><a class='page-link' href='javascript:MovePage(" + nextPage + ", " + nextMoveType + ", \"" + call_url + "\", \"" + area_data_id + "\");' aria-label='Next'><i class='fas fa-angle-right fa-lg'></i></a></li>";
		append_data += "<li class='page-item'><a class='page-link' href='javascript:MovePage(" + totalPageCount + ", " + nextMoveType + ", \"" + call_url + "\", \"" + area_data_id + "\");' aria-label='Next'><i class='fas fa-angle-double-right fa-lg'></i></a></li>";
	}
	else {
		append_data += "<li class='page-item disabled'><a class='page-link' aria-label='Next'><i class='fas fa-angle-right fa-lg'></i></a></li>";
		append_data += "<li class='page-item disabled'><a class='page-link' aria-label='Last'><i class='fas fa-angle-double-right fa-lg'></i></a></li>";
	}

	$("#" + _obj_paging).append(append_data);
}

/* ----------------------------------------------------------- */
//      함수명 : MovePage
//      설명 : 페이지 이동시 데이터 호출
//      파라미터 : 
//          1. page = 이동 페이지
//          2. moveType = 이동 여부
//          3. actionUrl = 요청 URL
//          4. formName = form 아이디
//          5. area_data_id = 데이터가 삽입 태그아이디
/* ----------------------------------------------------------- */
function MovePage(page, moveType, call_url, area_data_id) {
	$("#page").val(page);
	if (moveType === 0) {
		CallListData(call_url, area_data_id);
	}
}

// 리스트 호출
function GetList() {
	$("#page").val("1");
	CallListData(list_data_url, area_data_id);
}